#!/bin/gnuplot
set linetype 1  dashtype 1 linecolor rgb "#4477AA"
set linetype 2  dashtype 1 linecolor rgb "#CC6677"
set linetype 3  dashtype 1 linecolor rgb "#117733"
set linetype 4  dashtype 1 linecolor rgb "#DDCC77"
set linetype 5  dashtype 1 linecolor rgb "#88CCEE"
set linetype 6  dashtype 1 linecolor rgb "#AA4499"
set linetype 7  dashtype 1 linecolor rgb "#44AA99"
set linetype 8  dashtype 1 linecolor rgb "#999933"
set linetype 9  dashtype 1 linecolor rgb "#882255"
set linetype 10 dashtype 1 linecolor rgb "#332288"
set linetype 11 dashtype 1 linecolor rgb "#661100"
set linetype 12 dashtype 1 linecolor rgb "#AA4466"
set linetype cycle 12
set linetype 99 dashtype 1 linecolor rgb "#DDDDDD"

set palette defined ( 0 "#DDDDDD", \
                      1 "#4477AA", \
                      2 "#CC6677", \
                      3 "#DDCC77", \
                      4 "#117733", \
                      5 "#88CCEE", \
                      6 "#AA4499", \
                      7 "#44AA99", \
                      8 "#999933", \
                      9 "#882255", \
                     10 "#661100", \
                     11 "#332288", \
                     12 "#AA4466" )